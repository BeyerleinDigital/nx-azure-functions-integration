{
  "$schema": "http://json-schema.org/schema",
  "name": "azure-functions",
  "version": "0.0.1",
  "generators": {
    "create-project": {
      "factory": "./src/generators/create-project/generator",
      "schema": "./src/generators/create-project/schema.json",
      "description": "Generate an Azure Functions project."
